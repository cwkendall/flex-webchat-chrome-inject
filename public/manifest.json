{
  "manifest_version": 2,
  "name": "Deadlift for Flex",
  "version": "0.0.3",
  "description": "An extension to inject flex webchat into pages for demos",

  "browser_action": {
    "default_title": "Deadlift",
    "default_popup": "deadlift.html"
  },

  "permissions": ["activeTab", "tabs", "storage", "https://*/*", "http://*/*"]
}
