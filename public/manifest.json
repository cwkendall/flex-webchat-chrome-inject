{
  "manifest_version": 2,
  "name": "Deadlift for Flex",
  "version": "0.0.2",
  "description":"An extension to inject flex webchat into pages for demos",

  "browser_action":{
    "default_title": "Deadlift",
    "default_popup": "deadlift.html"
  },

  "permissions":["tabs", "storage", "https://*/*", "http://*/*"],

  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*", "file://*/*"],
      "css": ["./static/css/main.css"],
      "js": ["./assets/webchat-branding.js","./assets/webchat-appConfig.js", "./static/js/main.js"]
    }
  ]
}